{
  "name": "angularsignalrclient",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --output-path=dist/web",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-desktop": "npm run cleanup-desktop && npm run build-prod -- --base-href ./ && npm run desktop-copy-files && npm run pack-desktop",
    "desktop-copy-files": "npm run desktop-copy-app-files && npm run desktop-copy-desktop-files ",
    "desktop-copy-app-files": "copyfiles ./dist/web/**/*.* ./.temp/desktop -f",
    "desktop-copy-desktop-files": "copyfiles ../assets/desktop/**/*.* ./.temp/desktop -f",
    "pack-desktop": "electron-packager .temp/desktop/ --electronVersion=5.0.0 --icon=\".temp/desktop/icon\" --platform=win32,linux --out=./dist/desktop/",
    "cleanup-desktop": "npm run delete-temp-folder && npm run delete-dist-desktop-folder",
    "delete-temp-folder": "rimraf .temp/desktop",
    "delete-dist-desktop-folder": "rimraf dist/desktop"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.1",
    "@angular/cdk": "^10.0.0",
    "@angular/common": "~10.0.1",
    "@angular/compiler": "~10.0.1",
    "@angular/core": "~10.0.1",
    "@angular/forms": "~10.0.1",
    "@angular/platform-browser": "~10.0.1",
    "@angular/platform-browser-dynamic": "~10.0.1",
    "@angular/router": "~10.0.1",
    "@microsoft/signalr": "3.0.0",
    "@swimlane/ngx-charts": "14.0.0",
    "bootstrap": "^4.3.1",
    "electron-packager": "^13.1.1",
    "jquery": "^3.5.0",
    "rxjs": "~6.5.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.2",
    "rimraf": "^2.6.3",
    "copyfiles": "^2.1.0",
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.1",
    "@angular/language-service": "~10.0.1",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
